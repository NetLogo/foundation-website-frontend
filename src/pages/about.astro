---
import Layout from "../layouts/Layout.astro";
import { AboutSection } from "../components/about/about-page";
import NetLogoAPI from "../utils/api";

const api = new NetLogoAPI();
const aboutdata = await api.getAboutContent();
---

<Layout
    title="About NetLogo"
    description="NetLogo Abouts Page"
>
    <AboutSection AboutData={aboutdata} client:load />
</Layout>