---
import Layout from "../layouts/Layout.astro";
import { AboutSection } from "../components/about/about-page";
import NetLogoAPI from "../utils/api";

const api = new NetLogoAPI();
const campaignData = await api.getCampaignContent();
---

<Layout
    title="NetLogo Center Campaign"
    description="About The NetLogo Center Campaign"
>
    <AboutSection AboutData={campaignData} client:load />
</Layout>
